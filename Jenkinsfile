pipeline {
  agents any
  stages {
      stage('build') {
         steps {
            echo "this is build stage"
         }
      }
      stage('test') {
        steps {
           echo "this is test stage"
        }
      }
      stage('push') {
        steps {
           echo "this is push stage"
        }
      }
      stage('deploy'){
        steps{
           echo "this is deploy stage"
        }
      }
  }
}